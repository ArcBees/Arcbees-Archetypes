#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .root {
            display: inline;
        }
    </ui:style>
    
    <g:HTMLPanel styleName="{style.root}">
        <g:InlineHTML ui:field="nickname"/>
        <g:Anchor ui:field="link" />
    </g:HTMLPanel>
</ui:UiBinder>
